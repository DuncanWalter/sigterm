{
  "name": "sigterm",
  "version": "1.0.0",
  "description": "Card game inspired by Slay The Spire",
  "main": "src/core/index.js",
  "scripts": {
    "dev": "parcel ./src/index.html -d build/",
    "start-dev": "ELECTRON_START_URL=http://localhost:1234 electron ./src",
    "start": "electron .",
    "build": "parcel build ./src/index.html -d build/ --public-url ./",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "test": "test"
  },
  "keywords": [
    "card",
    "game",
    "deck",
    "rpg",
    "roguelike"
  ],
  "author": "Duncan Walter",
  "license": "MIT",
  "dependencies": {
    "electron": "^2.0.4",
    "kefir": "^3.8.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "styled-components": "^3.3.3"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2019": "^1.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-typescript": "^7.0.0-alpha.19",
    "electron-builder": "^20.19.2",
    "electron-reload": "^1.2.5",
    "file-loader": "^1.1.11",
    "flow-bin": "^0.76.0",
    "html-webpack-plugin": "^3.2.0",
    "parcel-bundler": "^1.9.4",
    "typescript": "^3.0.1"
  }
}
